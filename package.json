{
  "name": "learn-about-github",
  "version": "1.0.0",
  "private": false,
  "description": "hugo-reveal live slide deck",
  "main": "lib/main.js",
  "scripts": {
    "build": "hugo --cleanDestinationDir --minify --config config.toml  --destination=../docs --environment production --forceSyncStatic --gc --path-warnings --templateMetrics --templateMetricsHints --verbose --source=src/",
    "serve": "hugo server --cleanDestinationDir --config config.toml --environment production --forceSyncStatic --gc --path-warnings --templateMetrics --templateMetricsHints --verbose --source=src/",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charrismatic/learn-about-github"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
